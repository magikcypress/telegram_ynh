{
    "name": "Telegram",
    "id": "telegram",
    "packaging_format": 1,
    "requirements": {
        "yunohost": ">> 2.5.0"
    },
    "description": {
        "en": "Telegram - a new era of messaging",
        "fr": "Telegram - un nouvelle espace de messagerie"
    },
    "version": "0.5.3",
    "license": "GNU v3.0",
    "maintainer": {
        "name": "cyp",
        "email": "cyp@rouquin.me",
        "url": "http://www.rouquin.me"
    },
    "multi_instance": false,
    "url": "https://github.com/zhukov/webogram",
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "ask": {
                    "en": "Choose a domain for Telegram",
                    "fr": "Choisissez un domaine pour Telegram"
                },
                "example": "domain.org"
            },
            {
                "name": "path",
                "type": "path",
                "ask": {
                    "en": "Choose a path for Telegram",
                    "fr": "Choisissez un chemin pour Telegram"
                },
                "example": "/telegram",
                "default": "/telegram"
            }
        ]
    }
}